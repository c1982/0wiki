{
    "manifest_version": 2,  
    "name": "0wiki",
    "description": "This extension just replace wikipedia.org to 0wikipedia.org",
    "version": "1.0",  
    "browser_action": {
      "default_icon": "img/icon.png"      
    },    
    "author":"OÄŸuzhan YILMAZ <aspsrc@gmail.com>",    
    "homepage_url": "https://github.com/c1982/0wiki",
    "permissions": [            
      "webRequest",
      "webRequestBlocking",      
      "*://wikipedia.org/*",
      "*://*.wikipedia.org/*"
    ],
    "background": {"scripts": ["js/main.js"]}    
  }